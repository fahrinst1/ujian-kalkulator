    }                                      

    private void btnKurActionPerformed(java.awt.event.ActionEvent evt) {                                       
        angka1 =Double.parseDouble(angka);
        display.setText("-");
        angka="";
        pilih=2;
    }                                      

    private void Btnangka1ActionPerformed(java.awt.event.ActionEvent evt) {                                          
